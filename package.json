{
  "name": "@h7/compute-js-dynamic-build",
  "type": "module",
  "version": "0.1.1",
  "description": "",
  "main": "index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "fastly": {
        "types": "./build/fastly/index.d.ts",
        "default": "./build/fastly/index.js"
      },
      "default": {
        "types": "./build/node/index.d.ts",
        "default": "./build/node/index.js"
      }
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "build/**/*"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "npm run test && npm run build-package",
    "build-package": "npm run build-package:clean && npm run build-package:compile",
    "build-package:clean": "rimraf build",
    "build-package:compile": "tsc --build tsconfig.build.json",
    "coverage": "c8 npm test",
    "test": "npm run test:unit",
    "test:unit": "glob-bin -c \"node --loader ts-node/esm --enable-source-maps --no-warnings=ExperimentalWarning --test\" \"./test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harmony7/compute-js-dynamic-build.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/harmony7/compute-js-dynamic-build/issues"
  },
  "homepage": "https://github.com/harmony7/compute-js-dynamic-build#readme",
  "dependencies": {
    "@h7/js-async-pipeline": "^0.1.0",
    "@h7/http-import-esbuild-plugin": "^0.1.0",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "esbuild": "^0.27.0",
    "magic-string": "^0.30.21",
    "regexpu-core": "^6.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "c8": "^10.1.3",
    "glob-bin": "^1.0.0",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "esbuild": "^0.27.0"
  }
}
